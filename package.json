    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.1",
    "express-rate-limit": "^5.2.3",
    "helmet": "^4.3.1",
    "hpp": "^0.2.3",
    "mongoose": "^5.11.9",
    "morgan": "^1.10.0",
    "xss-clean": "^0.1.1"
